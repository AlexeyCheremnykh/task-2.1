include ../applicant/applicant.pug
include ../profile/profile.pug
include ../contact/contact.pug
include ../skills/skills.pug

mixin column-left({applicantData = {}, profileData = {}, contactData = {}, skillsData = {}} = {})
    .column-left
        .column-left__applicant
            +applicant(applicantData)
        .column-left__profile
            +profile(profileData)
        +contact(contactData)
        +skills(skillsData)
